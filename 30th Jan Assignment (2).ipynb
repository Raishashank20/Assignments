    "Ans (06)i = 1\n",
    "while i <= 4:\n",
    "    j= 0\n",
    "    while j <=3 :\n",
    "        print(i*j, end = \" \")\n",
    "        j +=1\n",
    "    print()  \n",
    "    i +=1"
   ]
